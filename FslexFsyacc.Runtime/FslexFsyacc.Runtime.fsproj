<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Description>Runtime for Fslex/Fsyacc analyzer/parser generation tools.</Description>
    <Copyright>xp44mm</Copyright>
    <PackageLicenseExpression>LGPL-3.0-or-later</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/xp44mm/FslexFsyacc</PackageProjectUrl>
    <RepositoryUrl>https://github.com/xp44mm/FslexFsyacc</RepositoryUrl>
    <PackageTags>lex,yacc</PackageTags>
    <PackageReleaseNotes>analyzer</PackageReleaseNotes>
    <Authors>xp44m</Authors>
    <Company>34696643@qq.com</Company>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Version>1.0.3</Version>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="StateContext.fs" />
    <Compile Include="TagLexicalAnalyzer.fs" />
    <Compile Include="LexicalAnalyzer.fs" />
    <Compile Include="Parser.fs" />
    <Compile Include="Analyzer.fs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="FSharp.Idioms" Version="1.1.16" />
    <PackageReference Include="FSharp.Literals" Version="2.2.5" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Update="FSharp.Core" Version="6.0.1" />
  </ItemGroup>

</Project>
